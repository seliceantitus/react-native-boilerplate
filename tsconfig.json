{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "react",
		"lib": [
			"es6"
		],
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"target": "esnext",
		"baseUrl": "./src",
		"paths": {
			"_assets": [
				"./assets"
			],
			"_components": [
				"./components"
			],
			"_config": [
				"./config"
			],
			"_screens": [
				"./screens"
			],
			"_models": [
				"./models"
			],
			"_navigators": [
				"./navigators"
			],
			"_services": [
				"./services"
			],
			"_theme": [
				"./theme"
			],
			"_utils": [
				"./utils"
			],
			"_actions": [
				"./actions"
			],
			"_reducers": [
				"./reducers"
			],
			"_sagas": [
				"./sagas"
			],
			"_store": [
				"./store"
			]
		}
	},
	"exclude": [
		"node@modules",
		"babel.config.js",
		"metro.config.js",
		"jest.config.js"
	]
}